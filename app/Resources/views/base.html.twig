<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="Site privé pour les élèves d'Arts et Métiers ParisTech">
        <meta name="keywords" content="phy'sbook, intranet, ensam, arts, métiers, paristech, site, privé, élèves, physbook, phy'ssbook, physsbook">
        <meta name="author" content="Louis Lagrange">
        <title>{% block title %}Phy'sbook{% endblock %}</title>
        {% block stylesheets %}
        {% stylesheets
            '%kernel.root_dir%/../app/Resources/public/less/general.less'
            '%kernel.root_dir%/../app/Resources/public/css/dataTables.bootstrap.css'
            '%kernel.root_dir%/../app/Resources/public/lib/select2/select2.css'
            '%kernel.root_dir%/../app/Resources/public/lib/select2-bootstrap-css/select2-bootstrap.css'
            '%kernel.root_dir%/../app/Resources/public/lib/webui-popover/dist/jquery.webui-popover.min.css'
            filter='less,cssrewrite,?yui_css'
            output='css/design.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% endblock %}
        <link rel="manifest" href="{{ asset('manifest.json') }}">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('images/favicon/apple-touch-icon-57x57.png') }}">
        <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('images/favicon/apple-touch-icon-114x114.png') }}">
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('images/favicon/apple-touch-icon-72x72.png') }}">
        <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('images/favicon/apple-touch-icon-144x144.png') }}">
        <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('images/favicon/apple-touch-icon-60x60.png') }}">
        <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('images/favicon/apple-touch-icon-120x120.png') }}">
        <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('images/favicon/apple-touch-icon-76x76.png') }}">
        <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('images/favicon/apple-touch-icon-152x152.png') }}">
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('images/favicon/apple-touch-icon-180x180.png') }}">
        <meta name="apple-mobile-web-app-title" content="Phy'sbook">
        <link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-192x192.png') }}" sizes="192x192">
        <link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-160x160.png') }}" sizes="160x160">
        <link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-96x96.png') }}" sizes="96x96">
        <link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-16x16.png') }}" sizes="16x16">
        <link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-32x32.png') }}" sizes="32x32">
        <meta name="msapplication-TileColor" content="#2b5797">
        <meta name="msapplication-TileImage" content="{{ asset('images/favicon/mstile-144x144.png') }}">
        <meta name="application-name" content="Phy'sbook">
        <meta name="theme-color" content="#b63938">
        {% block base_javascripts %}
        <script type="text/javascript">
            var assetsDir = "{{ asset('') }}";
        </script>
        {% endblock %}
    </head>
    <body{% if block('pageName') != "" %} class="{{ block('pageName') }}"{% endif %}>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
        <!-- Piwik -->
            <noscript><p><img src="//analytics.physbook.fr/piwik.php?idsite=1" style="border:0;" alt="" /></p></noscript>
        <!-- End Piwik Code -->
    </body>
</html>
