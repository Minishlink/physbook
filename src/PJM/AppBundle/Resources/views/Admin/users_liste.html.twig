<p>Il y a {{ users|length }} utilisateurs.</p>

<div class="table-responsive">
    <table class="table table-striped table-hover table-condensed">
        <tr>
            <th>Username</th>
            <th>Fam's</th>
            <th>Bucque</th>
            <th>E-mail</th>
            <th>Prénom</th>
            <th>Nom</th>
            <th>Genre</th>
            <th>Téléphone</th>
            <th>K'gib</th>
            <th>Classe</th>
            <th>Anniversaire</th>
        </tr>
        {% for user in users %}
        <tr>
            <td>{{ user.username }}</td>
            <td>{{ user.fams }}</td>
            <td>{{ user.bucque }}</td>
            <td>{{ user.email }}</td>
            <td>{{ user.prenom }}</td>
            <td>{{ user.nom }}</td>
            <td>{{ user.genre ? "F" : "M" }}</td>
            <td>{{ user.telephone }}</td>
            <td>{{ user.appartement }}</td>
            <td>{{ user.classe }}</td>
            <td>{{ user.anniversaire }}</td>
        </tr>
        {% endfor %}
    </table>
</div>
